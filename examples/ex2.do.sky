
function<int> multi(n : _int32) {
    n = n * n;
    ret n;
} 

function<void> main() {

    _int32 number = 0;

    ultra res = do<int> :: (n) -> {
        n = n * 2;
        ret n;
    }

    ultra res1 = res :: number;
    
    [loop] power <- (multi(value), 10);

    _int32 n = 4;

    do<power> :: (out) n {
        print("n over every iteration: %d", n);
    };
}